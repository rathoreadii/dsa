vector<int> Solution::solve(vector<int> &A) {
    vector<int>ans;
   
    for(int i=0;i<A.size();i++)
    {
        if(A[i]>0)
        {
            ans.push_back(A[i]);
        }
    }
    for(int i=0;i<A.size();i++)
    {
        if(A[i]==0)
        {
            ans.push_back(A[i]);
        }
    }
    return ans;
}
