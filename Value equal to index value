vector<int> valueEqualToIndex(vector<int>& arr) {
        // code here
        int n= arr.size();
        vector<int> ans;
        
        for(int i= 0; i<n; i++)
        {
            if(arr[i] == i+1)
            {
                // cout<<arr[i]<<" "<<i<<endl;
                ans.push_back(arr[i]);
            }
        }
        
        return ans;
    }
