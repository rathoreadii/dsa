/*Complete the function below*/

#include <vector>

class Solution {
public:

    bool isPalin(int num)
    {
        vector<int> digits;
        
        while(num > 0)
        {
            digits.push_back(num%10);
            num/= 10;
        }
        
        int len= digits.size();
        int l= 0, r= len-1;
        
        while(l <= r)
        {
            if(digits[l] == digits[r])
            {
                l++;
                r--;
            }
            else
                return false;
        }
        return true;
    }

    int PalinArray(int a[], int n)
    {
    	// code here
	    bool flag= true;
    	
    	for(int i=0; i<n; i++)
    	{
    	    flag= isPalin(a[i]);
    	    if(flag == false)
    	        return 0;
    	}
    	
    	return 1;
    	
    }
};

//{ Driver Code Starts.

int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int n;
		cin>>n;
		int a[n];
		for(int i = 0; i < n; i++)
			cin>>a[i];
		Solution obj;
		cout<<obj.PalinArray(a,n)<<endl;
	}
}
// } Driver Code Ends
